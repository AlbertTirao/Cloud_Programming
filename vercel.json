{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
      "NEWSAPI_KEY": "@newsapi_key",
      "OPEN_WEATHER_MAP": "@open_weather_map",
      "SPOTIFY_CLIENT_ID": "@spotify_client_id",
      "SPOTIFY_CLIENT_SECRET": "@spotify_client_secret",
      "SPOTIFY_REDIRECT_URI": "@spotify_redirect_uri"
    }
  }
  